<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="oval"  >
            <corners android:radius="30dip" />
            <stroke android:width="20dip"

                android:color="#ffff472a" />
            <gradient android:angle="0" android:startColor="#ffff7a2f" android:endColor="#ffff0c10"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="34dip" />
            <stroke android:width="1dip" android:color="#FDD835" />
            <solid android:color="#FFEB3B"/>
        </shape>
    </item>
    <item >
        <shape android:shape="oval"
            >
            <corners android:radius="30dip"
                />
            <stroke android:width="10dip"


                android:color="#ffff0c10" />
            <gradient android:angle="0" android:startColor="#ffff0c10" android:endColor="#ffff0c10" />
        </shape>
    </item>
</selector>